devtools::load_all()
folders <- c("~/xpht-tests/fingerprints")
col <- c("#000000", "#e69f00", "#009e73", "#d55e00")
for (folder in folders) {
inputDir <- paste(folder, "/xpht8", sep = "")
outputDir <- paste(folder, "/plots", sep = "")
files <- list.files(inputDir,
pattern = "*.RDS",
full.names = TRUE,
recursive = FALSE)
for (f in files) {
data <- readRDS(f)
par(mfrow = c(2,2), mai = c(0.6,0.6,0.6,0.6))
for (i in 1:length(data)) {
pltName <- head(strsplit(data[[i]][["Name"]], "-", fixed = TRUE)[[1]], -1)
numer <- as.character(i)
denom <- as.character(4)
pltName <- bquote(.(pltName) ~ .(numer) * pi / .(denom))
plot(data[[i]], barcode = TRUE, col = col, main = pltName)
}
}
}
